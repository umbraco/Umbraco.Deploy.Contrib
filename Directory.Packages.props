  </ItemGroup>
  <!-- Umbraco packages -->
  <ItemGroup>
    <PackageVersion Include="Umbraco.Cms.Web.Common" Version="[10.0.0, 11)" />
    <PackageVersion Include="Umbraco.Deploy.Infrastructure" Version="[10.4.0-rc1, 11)" />
  </ItemGroup>
</Project>
